{
  "expo": {
    "name": "convocations",
    "slug": "convocations",
    "version": "1.0.0",
    "scheme": "convocations",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/IITT-logo.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/IITT-logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iitt.convocations"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0c50456f-5cc4-4105-acdd-8d9a8a6d338f"
      }
    }
  }
}
